{
  "version": 2,
  "builds": [
    {
      "src": "public/index.php",
      "use": "@vercel/php"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "functions" : {
    "api/index.php": {
      "runtime": "@vercel/php@0.1.0",
      "includeFiles": "api/**/*"
    }
  }
}
